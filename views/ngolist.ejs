        <% for(var i=0;i<ngos.length;i++){ %>
            <div class="col-md-4 col-sm-6">
                <div class="thumbnail">
                    <img src=<%=ngos[i].image %> >
                    <div class="caption text-center">
                        <h3>
                            <%= ngos[i].name %>
                        </h3>
                        <a class="btn btn-info" href="/ngo/proposal" >Collaborate with us</a>

                    </div>
    
                </div>
            </div>
    
            <% } %>
    </div>
</div>
<%include ./partials/footer.ejs%>